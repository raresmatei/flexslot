{
  "name": "@flexslot/types",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --watch --dts --format esm,cjs"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "@types/node": "^22.7.4",
    "rimraf": "^6.0.1"
  }
}
